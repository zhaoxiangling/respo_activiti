  <!-- 查询 -->
  <select id="queryAllLogInfo" resultMap="BaseResultMap" parameterType="com.sxt.sys.vo.LogInfoVo" >
    select 
    <include refid="Base_Column_List" />
    from sys_log_login
   	<where>
   		<if test="loginname!=null">
